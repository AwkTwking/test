<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="com.baidu.nsfw.user.entity.User">
  <id name="id" type="java.lang.String">
   <column length="32" name="id"/>
   <generator class="uuid.hex"/>
  </id>
  <property generated="never" lazy="false" name="name" type="java.lang.String">
   <column length="20" name="name" not-null="true"/>
  </property>
  <property generated="never" lazy="false" name="dept" type="java.lang.String">
   <column length="20" name="dept" not-null="true"/>
  </property>
  <property generated="never" lazy="false" name="account" type="java.lang.String">
   <column length="50" name="account" not-null="true"/>
  </property>
  <property generated="never" lazy="false" name="password" type="java.lang.String">
   <column length="50" name="password" not-null="true"/>
  </property>
  <property generated="never" lazy="false" name="headImg" type="java.lang.String">
   <column length="100" name="headImg"/>
  </property>
  <property generated="never" lazy="false" name="gender" type="java.lang.Boolean">
   <column name="gender"/>
  </property>
  <property generated="never" lazy="false" name="email" type="java.lang.String">
   <column length="50" name="email"/>
  </property>
  <property generated="never" lazy="false" name="mobile" type="java.lang.String">
   <column length="20" name="mobile"/>
  </property>
  <property generated="never" lazy="false" name="birthday" type="java.util.Date">
   <column length="10" name="birthday"/>
  </property>
  <property generated="never" lazy="false" name="state" type="java.lang.String">
   <column length="1" name="state"/>
  </property>
  <property generated="never" lazy="false" name="memo" type="java.lang.String">
   <column length="200" name="memo"/>
  </property>
 </class>

</hibernate-mapping>
